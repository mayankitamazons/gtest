    <section class="contact-section spad">
      <div class="container">
        <div class="row">
          <div class="col-lg-6 offset-lg-1">
            <div class="contact-form">
              <div class="leave-comment">
                <h4>Any suggestion?</h4>
                <p>We will surely surely accept your suugestion</p>
                <form
                  action="/user/suggestion_submit"
                  method="post"
                  class="comment-form"
                >
                  <div class="row">
                    <div class="col-lg-6">
                      <input type="text" name="name" placeholder="Your name" />
                    </div>
                    <div class="col-lg-6">
                      <input
                        type="email"
                        name="email"
                        placeholder="Your email"
                      />
                    </div>
                    <div class="col-lg-12">
                      <textarea
                        name="suggest"
                        placeholder="Your suggestion"
                      ></textarea>
                      <button type="submit" class="site-btn">Send</button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
