    <!-- Shopping Cart Section Begin -->
    <section class="shopping-cart spad">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="cart-table">
              <table>
                <thead>
                  <tr>
                    <th>Order Id</th>
                    <th>Product</th>
                    <th>Customer Name</th>
                    <th>Mobile no.</th>
                    <th>Address</th>
                    <th>Total</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <td><%=data.id%></td>
                    <td>
                      <% products.forEach(function(item){ %> <%=item.name%><br />
                      <img
                        width="50px"
                        height="50px"
                        src="/images/<%= item.image%>"
                      /><br />
                      <%})%>
                    </td>
                    <td><%=data.name%></td>
                    <td><%=data.mobile%></td>
                    <td><%=data.address%></td>
                    <td><%=data.total%></td>
                    <td>
                      <% if (data.status_code==100) { %>
                      <p>pending</p>
                      <% } else if(data.status_code==200){ %>
                      <p>assigned</p>
                      <% } else if (data.status_code==300) { %>
                      <p>picked</p>
                      <% } else if(data.status_code==400){ %>
                      <p>on the way</p>
                      <% } else if (data.status_code==500) { %>
                      <p>delievered</p>
                      <% } else if (data.status_code==600){ %>
                      <p>cancelled</p>
                      <% } %>
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>
