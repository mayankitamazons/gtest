                <div class="col-lg-12">
                  <label for="street">Street Address<span>*</span></label>
                  <input
                    type="text"
                    name="address"
                    class="street-first"
                    required
                  />
                </div>
                <!-- <div class="col-lg-12">
                                <label for="zip">Postcode / ZIP (optional)</label>
                                <input type="text" id="zip">
                            </div>
                            <div class="col-lg-12">
