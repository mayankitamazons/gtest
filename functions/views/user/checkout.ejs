                <div class="col-lg-6">
                  <label for="phone">Phone<span>*</span></label>
                  <input type="text" name="mobile" required />
                </div>
                
              </div>
            </div>
            <div class="col-lg-6">
              <!-- <div class="checkout-content">
                <input type="text" placeholder="Enter Your Coupon Code" />
              </div> -->
              <div class="place-order">
                <h4>Your Order</h4>
                <div class="order-total">
                  <ul class="order-table">
                    <li>Product <span>Total</span></li>
                    <% let total = 0 %>
                    <% let total2 = 0 %>
                   <% cart.forEach(function(item){%> <%
                    total = total + parseFloat(item.sp*item.qty)%>
                    <li class="fw-normal">
                        <% total2 = parseFloat(item.sp*item.qty) %>
                      <%=item.name%> x <%=item.qty%> <span>Rs.<%=total2%></span>
                    </li>
                    <%})%>
                  </ul>
                  <div class="payment-check">
                    <div class="pc-item">
                      <label for="pc-check">
                        Cash on delievery
                        <input type="checkbox" id="pc-check" />
                        <span class="checkmark"></span>
                      </label>
                    </div>
                    <div class="pc-item">
                      <label for="pc-paypal">
                        UPI
                        <input type="checkbox" id="pc-paypal" />
                        <span class="checkmark"></span>
                      </label>
                    </div>
                    <div class="pc-item">
                      <label for="pc-paypal">
                        Debit/Credit
                        <input type="checkbox" id="pc-paypal" />
                        <span class="checkmark"></span>
                      </label>
                    </div>
                  </div>
                  <div class="order-btn">
                    <button type="submit" class="site-btn place-btn">
                      Place Order
                    </button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </form>
