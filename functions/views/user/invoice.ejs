    <!-- Shopping Cart Section Begin -->
    <% data.forEach(function(item){ %>
    <div class="card">
      <div class="card-body">
        <div class="invoice-area">
          <div class="invoice-head">
            <div class="row">
              <div class="iv-left col-6">
                <span>INVOICE</span>
              </div>
              <!-- <div class="iv-right col-6 text-md-right">
                <span>#34445998</span>
              </div> -->
            </div>
          </div>
          <div class="row align-items-center">
            <div class="col-md-6">
              <div class="invoice-address">
                <h3>Invoiced to</h3>
                <h5><%=item.name%></h5>
                <p><%=item.address%></p>
              </div>
            </div>
            <div class="col-md-6 text-md-right">
              <ul class="invoice-date">
                <li>Invoice Date : <%=item.date%></li>
                <li>Invoice Time : <%=item.time%></li>
              </ul>
            </div>
          </div>
          <div class="invoice-table table-responsive mt-5">
            <table class="table table-bordered table-hover text-right">
              <thead>
                <tr class="text-capitalize">
                  <th class="text-center" style="width: 5%;">Order id</th>
                  <th class="text-center">Image</th>
                  <th class="text-center" style="width: 45%; min-width: 130px;">
                    Product
                  </th>
                  <th class="text-center" style="width: 45%; min-width: 130px;">
                    Price
                  </th>
                  <th class="text-center">total</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td class="text-center"><%=item.id%></td>
                  <td class="text-center">
                    <% products.forEach(function(item2){ %> <% if
                    (item.pro_ids.includes(item2.id)) { %>
                    <img
                      width="50px"
                      height="50px"
                      src="/images/<%= item2.image%>"
                    /><br />
                    <% } %> <%})%>
                  </td>
                  <td class="text-center">
                    <% products.forEach(function(item2){ %> <% if
                    (item.pro_ids.includes(item2.id)) { %> <%=item2.name%><br />
                    <% } %> <%})%>
                  </td>

                  <% let a = item.pro_qty; a = a.replace("(", "["); a =
                  a.replace(")", "]"); a = JSON.parse(a); %>

                  <td class="text-center">
                    <% a.forEach(function(item2){ %><%=item2%><br />
                    <%})%>
                  </td>
                  <td class="text-center"><%=item.total%></td>
                </tr>
              </tbody>
            </table>
            <form action="/user/feedback" method="post" class="comment-form">
              <button
                type="submit"
                name="order_id"
                value="<%=item.id%>"
                class="site-btn"
              >
                FEEDBACK
              </button>
            </form>
          </div>
        </div>
        <!-- <div class="invoice-buttons text-right">
                    <a href="#" class="invoice-btn">print invoice</a>
                    <a href="#" class="invoice-btn">send invoice</a>
                </div> -->
      </div>
    </div>
    <%})%>
